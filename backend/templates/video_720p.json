{
  "yt-dlp": {
    "--sleep-interval": "1-3",
    "--max-sleep-interval": "5",
    "--sleep-requests": "1-2",
    "--throttled-rate": "100K",
    "--newline": true,
    "--ignore-errors": true,
    "--retries": "3",
    "--extractor-retries": "2",
    "--fragment-retries": "3",
    "--no-mtime": true,
    "--continue": true,
    "--progress": true,
    "--console-title": true,
    "--match-filters": "duration>300 & duration<14400",
    "--reject-title": "trailer|promo|premierelive|stream",
    "--format": "bestvideo[height=?720]+bestaudio/best[height=?720]/bestvideo[height<=?1080]+bestaudio/best[height<=?1080]/bestvideo[height<=?720]+bestaudio/best[height<=?720]/bestvideo+bestaudio/best",
    "--output": "%(upload_date)s - %(title)s.%(ext)s",
    "--download-archive": "/path/to/user/folder/configs/ytdl-archive.txt",
    "--verbose": false,
    "--js-runtime": "deno:/path/to/deno",
    "--remote-components": "ejs:github",
    "--compat-options": "no-youtube-unavailable-videos",
    "--write-info-json": true,
    "--write-thumbnail": true,
    "--embed-thumbnail": true,
    "--embed-metadata": true,
    "--add-metadata": true,
    "--convert-thumbnails": "jpg",
    "--write-link": true,
    "--write-description": true,
    "--break-on-existing": false,
    "--keep-video": true,
    "--write-subs": true,
    "--write-auto-subs": true,
    "--embed-subs": true,
    "--subtitleslangs": "ar,en,tr,no",
    "--sub-format": "srt/ass/vtt/best"
  },
  "custom": {
    "--poster": false,
    "--random-agent": false,
    "--download-timeout": 7200,
    "--stall-timeout": 300
  }
}
